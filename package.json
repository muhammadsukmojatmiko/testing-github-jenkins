{
  "name": "backoffice-logistic-web",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=18.12.0"
  },
  "scripts": {
    "dev": "./generate_dotenv.sh development && next dev",
    "mock": "NEXT_PUBLIC_USE_MOCK_DATA=true next dev",
    "build": "next build",
    "build-production": "./generate_dotenv.sh production && yarn clean-build",
    "build-staging": "./generate_dotenv.sh staging && yarn clean-build",
    "build-static": "yarn build && yarn export",
    "clean-build": "rm -rf .next && rm -rf out && yarn build",
    "code-coverage": "jest --coverage",
    "export-static": "next build && next export",
    "lint": "next lint",
    "start": "next start",
    "test": "jest -c __test__/jest.config.js --passWithNoTests",
    "deploy-staging": "./generate_dotenv.sh staging && yarn clean-build && yarn export-static",
    "deploy-production": "./generate_dotenv.sh production && yarn clean-build && yarn export-static",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build"
  },
